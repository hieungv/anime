<% @movies.each do |movie| %>
  <div class="movie">
    <div class="movie-image"> <span class="play">
      <span class="name"><%= movie.name %></span>
        </span><a href="#"><%=image_tag "#{movie.image}", alt: "" %></a>
    </div>
    <div class="rating">
      <p>RATING</p>
      <div class="stars">
        <div class="stars-in"> </div>
      </div>
      <span class="comments">12</span>
    </div>
  </div>
<% end %>
